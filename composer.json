{
  "name": "eframe/amqp",
  "description": "AMQP wrapper for Laravel and Lumen to publish and consume messages",
  "keywords": [
    "laravel",
    "lumen",
    "laravel5",
    "rabbitmq",
    "amqp",
    "queue",
    "package",
    "message queue"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nazarii Makar",
      "email": "nazar.makar@magnise.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "php-amqplib/php-amqplib": "2.*",
    "illuminate/support": "5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "EFrame\\Amqp\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EFrame\\Amqp\\AmqpServiceProvider"
      ],
      "aliases": {
        "Amqp": "EFrame\\Amqp\\Facades\\Amqp"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
